@import '/pages/booklist/booklist-template';
@import '/pages/singlebook/singlebook-template';
@import '/pages/bookgood/bookgood-template';
.booktype{
  width:100%;
  display: flex;
  align-items: center;

}
.icon{
  width:30px;
  height:15px;
  margin-top:15rpx;
  margin-left: -10rpx;
}
.guessword{
  font-size: 15px;
  margin-top: 15rpx;

}
.popwords{
  width:100%;
  height:80rpx;
  font-size: 14px;
  text-align: center;
  line-height: 80rpx;
  color:#6fa8dc;
  border-bottom: 5px solid #FAF0E6;
}
.togglesex{
  color: white;
  width:75rpx;
  height:36rpx;
  font-size: 12px;
  margin-left: 450rpx;
  margin-top:10rpx;
  background: #6fa8dc;
  border: 1px solid #6fa8dc;
  border-radius: 50rpx;
  text-align: center;
  line-height: 36rpx;
}
.booklist{
  width:100%;
  display: flex;
  font-size: 13px;
  color: #666;
  border-bottom: 1px solid #FAF0E6;
  padding-bottom: 10rpx;
  flex-wrap: wrap;
  justify-content: space-between;
}
